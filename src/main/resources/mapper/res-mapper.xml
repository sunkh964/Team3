<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 해당 파일에 모든 쿼리문을 작성 -->
<mapper namespace="resMapper">
	<resultMap id="res" type="com.green.Team3.res.vo.ResVO">
        <id column="RES_NUM" property="resNum" />
        <result column="RES_TIME" property="resTime" />
        <collection property="memId" resultMap="memberMapper.member" />
        <collection property="staffNum" resultMap="staffMapper.staff" />
        <collection property="partNum" resultMap="staffMapper.part" />
    </resultMap>
</mapper>